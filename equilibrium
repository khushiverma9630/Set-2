#include <iostream>
using namespace std;
// equilibrium index in the array
int main()
{
    int i,j,n,k;
    cout<<"enter the value of size of array";
    cin>>n;
    int a[10];
    for(j=0;j<n;j++)
    {
        cin>>a[i];
    }
    int sum=0;
    for(i=0;i<n/2;i++)
    {
        sum=sum+a[i];
        
    }
    int sum1=0;
    
    for(j=n-1;j>i+1;j--)
    {
        sum1=sum1+a[i];
    }
    
    if(sum==sum1)
    {
        cout<<"the elliquiruibm index is "<<i<<endl;
        cout<<"the elliquiruibm elemant is "<<a[i]<<endl;
    }
    else 
    {
        cout<<-1;
    }
    return 0;
}
